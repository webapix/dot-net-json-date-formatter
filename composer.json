{
    "name": "webapix/dot-net-json-date-formatter",
    "description": "Convert .net json date format to PHP DateTime object and vica versa",
    "keywords": [
        "webapix",
        ".net json date parser",
        ".net json date converter"
    ],
    "homepage": "https://github.com/webapix/dot-net-json-date-formatter",
    "license": "MIT",
    "authors": [
        {
            "name": "Zsolt Kaveczki",
            "email": "zsocakave@webapix.hu",
            "homepage": "https://webapix.hu/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.27|^6|^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Webapix\\DotNetJsonDate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webapix\\DotNetJsonDate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
